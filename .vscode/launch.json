    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "preLaunchTask": "build-all",
            "program": "${workspaceFolder}/dist/server/server-ut.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "back office Fast Launch",
            "env": {"BACKEND_PLUS_LOCAL_CONFIG":"local-config.yaml"},
            "program": "${workspaceFolder}/dist/server/server-ut.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "DM Fast Launch",
            "env": {"BACKEND_PLUS_LOCAL_CONFIG":"local-config-dm.yaml"},
            "program": "${workspaceFolder}/dist/server/server-ut.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dumpDb",
            "preLaunchTask": "build-all",
            "program": "${workspaceFolder}/dist/server/server-ut.js",
            "args": [
                "--dump-db"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Fast dumpDb",
            "program": "${workspaceFolder}/dist/server/server-ut.js",
            "args": [
                "--dump-db"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}